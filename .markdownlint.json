{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "MD013": false,
  "MD033": {
    "allowed_elements": [
      "antml:*",
      "commentary",
      "example",
      "user",
      "response",
      "antml:function_calls",
      "antml:invoke",
      "antml:parameter"
    ]
  },
  "MD024": {
    "siblings_only": true
  },
  "MD041": false,
  "MD049": false,
  "MD050": false,
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD004": {
    "style": "dash"
  },
  "MD032": true
}
